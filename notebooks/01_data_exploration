   "source": [
    "# Identify categorical and numerical columns\n",
    "cat_cols = X.select_dtypes(include='object').columns\n",
    "num_cols = X.select_dtypes(exclude='object').columns\n",
    "\n",
    "len(cat_cols), len(num_cols)\n"
   ]
